
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User{
    id String @id @default(cuid())
    email String @unique
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}